{
  "manifest_version": 2,
  "name": "MicrosoftLearnSandboxRemover",
  "version": "1.0.0.4",
  "icons": {
    "128" : "icon.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": [ "content.js"]
    }
  ],

  "browser_action": {
    "default_title": "MSLearnSandboxRemover",
    "default_icon": "icon.png"
  },
  "web_accessible_resource": [],
  "permissions": ["https://docs.microsoft.com/*"]
}
